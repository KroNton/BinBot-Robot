<?xml version="1.0"?>

<launch>

  <!-- Run the map server -->
  <arg name="map_file" default="$(find binbot_navigation)/maps/map.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!--- Run AMCL -->
  <include file="$(find binbot_navigation)/launch/amcl.launch" />

  <!--- Run Move Base -->
  <include file="$(find binbot_navigation)/launch/move_base.launch" />

  <!--- Run Move Base -->
  <include file="$(find binbot_navigation)/launch/view_navigation.launch" />

  <!--- Run Move Base -->
  <include file="$(find binbot_navigation)/launch/3dsensor_sim.launch" />

</launch>
